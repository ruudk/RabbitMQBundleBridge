{
    "name": "simple-bus/rabbitmq-bundle-bridge",
    "type": "library",
    "description": "Use OldSoundRabbitMQBundle with SimpleBus/AsynchronousBundle",
    "keywords": ["RabbitMQ", "messages", "message bus", "event bus", "command bus"],
    "homepage": "http://github.com/SimpleBus/RabbitMQBundleBridge",
    "license": "MIT",
    "authors": [
        {
            "name": "Matthias Noback",
            "email": "matthiasnoback@gmail.com",
            "homepage": "http://php-and-symfony.matthiasnoback.nl"
        }
    ],
    "require": {
        "php": ">=5.4",
        "beberlei/assert": "~2.0",
        "simple-bus/message-bus": "~2.0",
        "simple-bus/asynchronous-bundle": "~2.0",
        "simple-bus/asynchronous": "~2.0",
        "oldsound/rabbitmq-bundle": "~1.0",
        "symfony/monolog-bundle": "~2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "simple-bus/jms-serializer-bundle-bridge": "~2.0",
        "symfony/console": "~2.0",
        "symfony/finder": "~2.0",
        "symfony/process": "~2.0",
        "matthiasnoback/phpunit-asynchronicity": "~1.0",
        "satooshi/php-coveralls": "~0.6"
    },
    "autoload": {
        "psr-4" : { "SimpleBus\\RabbitMQBundleBridge\\" : "src" }
    },
    "autoload-dev": {
        "psr-4": { "SimpleBus\\RabbitMQBundleBridge\\Tests\\": "tests" }
    }
}
